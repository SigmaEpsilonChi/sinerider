{
	"version": "0.0.1",
	"expressions": [
		"url = \"/worlds/move_wave.sinerider\"",
		"make_sledder(n) = {o: \"sled\", p: n*pi*4}",
		"sledders = map(range(-1, 1), make_sledder)",
		"g(n) = {o:\"goal\", p: (n-1/2)*pi*4-sin((n+1/2)*pi*4)*i, note: 28+7*floor(n/2)-2n, color: d3.interpolatePlasma((n+1)/(2))}",
		"goals = map(range(-1, 1), g)",
		"title={o:\"text\", p: 4i, v:\"Waves can be stretched too\", color: 0}",
		"// Try stretching this wave`",
		"Y=sin(`x`)"
	]
}
